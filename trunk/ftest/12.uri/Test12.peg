# Grammar used to validate host name URIs with no query or fragment clauses.# Based on <http://labs.apache.org/webarch/uri/rfc/rfc3986.html>.start = urivalue = voidnamespace = FTest12debug = uri := scheme ':' hier-part;scheme := alpha (alpha / digit / '+' / '-' / '.' )*;hier-part := '//' authority path-abempty;authority := (userinfo '@')? host (':' port)?;userinfo := (unreserved / pct-encoded / sub-delims / ':')*;host := reg-name;port := digit*;reg-name := (unreserved / pct-encoded / sub-delims)*;path-abempty := ('/' segment)*;segment := pchar*;pchar := unreserved / pct-encoded / sub-delims / ':' / '@';unreserved := alpha / digit / '-' / '.' / '_' / '~';pct-encoded := '%' hexdig hexdig;sub-delims := '!' / '$' / '&' / '\'' / '(' / ')' / '*' / '+' / ',' / ';' / '=';alpha := [a-zA-Z];digit := [0-9];hexdig := [a-fA-F0-9];