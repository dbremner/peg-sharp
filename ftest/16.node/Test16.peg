start = Exprvalue = XmlNodeusing = System.Linqexclude-methods = Parsedebug = noneExpr := S Sum;Sum := Product (('+' / '-') S Product)*;Product := Value (('*' / '/') S Value)*;Value := [0-9]+ S `value.InnerText = text.Trim()` `expected = "number"`Value := '(' Expr ')' S `;` `expected = "parenthesized expression"`Value := '"' . '"' S `value.InnerText = new string(text[1], 1)` `expected = "character"`S := Space* `text = null`  # We use a separate space rule because x* always succeeds.Space := [ \t\r\n] `;` `expected = "whitespace"`